<h1>Edit Post</h1>
  <%= form_for :post, :method => :put do |f| %>
    Title: <%= f.text_field :title, :value => @post.title %><br>
    City: <%= f.text_field :city, :value => @post.city%><br>
    Content: <%= f.text_field :content, :value => @post.content %><br>
    <%= f.hidden_field :user_id, :value => @post.user_id %>
    <%= f.submit %>
  <% end %>
